type ViewQueryResultResponse {
    fieldNames: [String]
    content: [[Object]]
    orderList: [Order]
    paging: Paging
}
type ViewQueryResult {
    fieldList: [ViewField]
    rowList: [[Object]]
    orderList: [Order]
    paging: Paging
}

type Order {
    fieldName: String
    direction: SortDirection
}

type Paging {
    totalElements: Long
    offset: Long
    hasMoreElements: Boolean
}